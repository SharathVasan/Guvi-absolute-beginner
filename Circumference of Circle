const readline = require('readline');
const inp = readline.createInterface ({
    input: process.stdin
});
const userInput = [];
inp.on("line", (data) => {
    userInput.push(data);
});
inp.on("close",() => {
  //start here
  var a = userInput[0].split(" ");
  var b = (2*(22/7)*+a).toFixed(2);
  console.log(b);
  //end here
});
